<tr>
  <th scope="row">
    <%= user.id %>
  </th>
  <td><%= user.first_name %> <%= user.last_name %></td>
  <td><%= user.birtday %></td>
  <td><%= user.email %></td>
  <td><%= user.password %></td>
  <td><%= user.exam_id %></td>
  <td>
    <%= link_to 'Update', edit_user_path(user), class: 'btn btn-warning' %>
    <%= link_to 'Delete', user, method: :delete, data: { confirm: 'Are you sure?' },
                                                          class: 'btn btn-danger'%>
  </td>
</tr>